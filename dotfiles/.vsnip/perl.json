{
	"Template": {
		"prefix": ["template"],
		"body": [
			"use strict;",
			"use warnings;",
			"",
			"$1"
		],
		"description": "Get input values."
	},
	"For Else": {
		"prefix": ["fore"],
		"body": [
			"for (my $$i=0; $$i<${1}; $$i++) {",
			"\t$2",
			"} else {",
			"\t$3",
			"}"
		],
		"description": "For statement."
	},
	"Input": {
		"prefix": ["input"],
		"body": ["my @input = glob<>;"],
		"description": "Get input values."
	},
	"MLog": {
		"prefix": ["mlog"],
		"body": [
			"use MLog;",
			"use Date::Dumper;",
			"MLog::write('$_::LOG_DIR/murakami', Dumper $1)"
		],
		"description": "Dump log."
	},
	"Sort": {
		"prefix": ["sort"],
		"body": ["my @sorted = sort{$$a <=> $$b} $1"],
		"description": "Sort by a value of int."
	},
	"SortHash": {
		"prefix": ["sorthash"],
		"body": ["my @sorted = sort{$$a->{$1} <=> $$b->{$1}} $2"],
		"description": "Sort by a value of hash."
	},
	"SortLength": {
		"prefix": ["sortlength"],
		"body": ["my @sorted = sort{length $$a cmp length $$b} $1"],
		"description": "Sort by a length of string."
	}
}
