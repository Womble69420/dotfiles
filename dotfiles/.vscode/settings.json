{
	"breadcrumbs.enabled": false,
	"cSpell.userWords": [
		"Vuex",
		"bufread",
		"firestore",
		"nuxt",
		"pathspec",
		"refname",
		"revparse",
		"socketio",
		"vuexfire",
		"xargs"
	],
	"editor.tabSize": 8,
	"editor.fontSize": 17,
	"editor.wordWrap": "on",
	"editor.formatOnSave": true,
	"editor.tabCompletion": "on",
	"editor.insertSpaces": false,
	"editor.minimap.size": "fill",
	"editor.smoothScrolling": true,
	"editor.cursorBlinking": "solid",
	"editor.renderWhitespace": "all",
	"editor.detectIndentation": false,
	"editor.renderLineHighlight": "all",
	"editor.fontFamily": "Hack Nerd Font",
	"editor.acceptSuggestionOnEnter": "on",
	"editor.minimap.renderCharacters": false,
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": true
	},
	"emmet.includeLanguages": {
		"javascript": "javascriptreact",
		"typescript": "typescriptreact",
		"vue-html": "html"
	},
	"eslint.options": {
		"configFile": "~/.eslintrc.json"
	},
	"eslint.probe": [
		"javascript",
		"javascriptreact",
		"typescript",
		"typescriptreact",
		"html",
		"vue",
		"markdown",
		"json"
	],
	"files.autoSave": "afterDelay",
	"files.trimFinalNewlines": true,
	"files.autoGuessEncoding": true,
	"files.trimTrailingWhitespace": true,
	"files.exclude": {
		"**/node_modules": true
	},
	"javascript.format.semicolons": "insert",
	"terminal.external.osxExec": "iTerm.app",
	"terminal.integrated.fontSize": 17,
	"terminal.integrated.cursorStyle": "line",
	"terminal.integrated.fontFamily": "Hack Nerd Font",
	"terminal.integrated.shell.osx": "/usr/local/bin/zsh",
	"workbench.colorTheme": "Ayu Dark",
	"workbench.iconTheme": "material-icon-theme",
	"workbench.colorCustomizations": {
		"sideBar.foreground": "#626A73",
		"sideBarTitle.foreground": "#626A73",
		"sideBarSectionHeader.foreground": "#626A73",
		"list.activeSelectionForeground": "#F0F0F0",
		"list.inactiveSelectionForeground": "#F0F0F0",
		"tab.activeForeground": "#F0F0F0",
		"tab.focusForeground": "#F0F0F0",
		"tab.inactiveForeground": "#626A73",
		"activityBar.foreground": "#626A73",
		"input.foreground": "#626A73",
		"input.placeholderForeground": "#626A73",
		"dropdown.foreground": "#626A73",
		"panelTitle.activeForeground": "#F0F0F0",
		"panelTitle.inactiveForeground": "#626A73",
		"statusBar.foreground": "#626A73",
		"breadcrumb.foreground": "#626A73",
		"foreground": "#626A73",
		"editorLineNumber.foreground": "#626A73",
		"editorLineNumber.activeForeground": "#F0F0F0"
	},
	"[go]": {
		"editor.codeActionsOnSave": {
			"source.organizeImports": true
		}
	},
	"go.alternateTools": {
		"go-langserver": "gopls"
	},
	"go.buildOnSave": "package",
	"go.formatTool": "goimports",
	"go.formatFlags": ["-w"],
	"go.gocodeAutoBuild": true,
	"go.installDependenciesWhenBuilding": true,
	"go.languageServerExperimentalFeatures": {
		"hover": true,
		"format": true,
		"rename": true,
		"diagnostics": true,
		"autoComplete": true,
		"signatureHelp": true,
		"goToDefinition": true,
		"findReferences": true,
		"documentSymbols": true,
		"workspaceSymbols": true,
		"goToTypeDefinition": true,
		"goToImplementation": true
	},
	"go.lintTool": "golint",
	"go.lintOnSave": "package",
	"gopls": {
		"deepCompletion": true,
		"enhancedHover": false,
		"usePlaceholders": false,
		"watchFileChanges": true,
		"completeUnimported": true
	},
	"go.testOnSave": false,
	"go.useLanguageServer": true,
	"go.vetOnSave": "package",
	"rust-client.channel": "stable",
	"prettier.tabWidth": 8,
	"prettier.useTabs": true,
	"prettier.singleQuote": true,
	"prettier.jsxSingleQuote": true,
	"prettier.jsxBracketSameLine": true,
	"prettier.vueIndentScriptAndStyle": true,
	"workbench.tips.enabled": false,
	"workbench.settings.editor": "json",
	"workbench.statusBar.visible": true,
	"workbench.fontAliasing": "antialiased",
	"workbench.editor.closeOnFileDelete": true,
	"window.zoomLevel": -2
}
