{
        "editor.fontSize": 12,
        "editor.fontFamily": "Noto Mono for Powerline",
        "workbench.colorTheme": "Ayu Dark",
        "workbench.colorCustomizations": {
                "sideBar.foreground": "#626A73",
                "sideBarTitle.foreground": "#626A73",
                "sideBarSectionHeader.foreground": "#626A73",
                "list.activeSelectionForeground": "#F0F0F0",
                "list.inactiveSelectionForeground": "#F0F0F0",
                "tab.activeForeground": "#F0F0F0",
                "tab.focusForeground": "#F0F0F0",
                "tab.inactiveForeground": "#626A73",
                "activityBar.foreground": "#626A73",
                "input.foreground": "#626A73",
                "input.placeholderForeground": "#626A73",
                "dropdown.foreground": "#626A73",
                "panelTitle.activeForeground": "#F0F0F0",
                "panelTitle.inactiveForeground": "#626A73",
                "statusBar.foreground": "#626A73",
                "breadcrumb.foreground": "#626A73",
                "foreground": "#626A73",
                "editorLineNumber.foreground": "#626A73",
                "editorLineNumber.activeForeground": "#F0F0F0",
        },
        "workbench.iconTheme": "material-icon-theme",
        "files.autoSave": "afterDelay",
        "editor.acceptSuggestionOnEnter": "off",
        "git.enabled": false,
        "diffEditor.renderSideBySide": false,
        "editor.colorDecorators": false,
        "editor.formatOnPaste": true,
        "editor.formatOnSave": true,
        "editor.formatOnType": true,
        "editor.minimap.renderCharacters": false,
        "editor.minimap.showSlider": "always",
        "editor.multiCursorModifier": "ctrlCmd",
        "editor.renderControlCharacters": true,
        "editor.renderLineHighlight": "all",
        "editor.renderWhitespace": "all",
        "editor.detectIndentation": false,
        "editor.tabSize": 8,
        "editor.wordWrap": "on",
        "files.associations": {
                ".*lintrc": "json"
        },
        "html.format.contentUnformatted": "pre, code, textarea, title, h1, h2, h3, h4, h5, h6, p",
        "html.format.extraLiners": "",
        "html.format.unformatted": null,
        "html.format.wrapLineLength": 0,
        "workbench.editor.labelFormat": "short",
        "workbench.editor.tabSizing": "shrink",
        "C_Cpp.updateChannel": "Insiders",
        "editor.suggest.snippetsPreventQuickSuggestions": false,
        "files.autoGuessEncoding": true,
        "window.zoomLevel": -1,
        "emmet.includeLanguages": {
                "javascript": "javascriptreact",
                "vue-html": "html"
        },
        "[go]": {
                "editor.tabSize": 4,
                "editor.insertSpaces": true,
                "editor.formatOnSave": true,
                "editor.formatOnPaste": false,
                "editor.formatOnType": false,
                "editor.codeActionsOnSave": {
                        "source.organizeImports": true
                },
        },
        "go.formatTool": "goreturns",
        "go.formatFlags": [
                "-w"
        ],
        "go.lintTool": "golint",
        "go.lintOnSave": "package",
        "go.vetOnSave": "package",
        "go.buildOnSave": "package",
        "go.testOnSave": false,
        "go.gocodeAutoBuild": true,
        "go.installDependenciesWhenBuilding": true,
        "go.useLanguageServer": true,
        "go.alternateTools": {
                "go-langserver": "gopls"
        },
        "go.languageServerExperimentalFeatures": {
                "format": true,
                "autoComplete": true,
                "rename": true,
                "goToDefinition": true,
                "hover": true,
                "signatureHelp": true,
                "goToTypeDefinition": true,
                "goToImplementation": true,
                "documentSymbols": true,
                "workspaceSymbols": true,
                "findReferences": true,
                "diagnostics": true
        },
        "gopls": {
                "usePlaceholders": false,
                "enhancedHover": false,
        },
        "explorer.confirmDragAndDrop": false,
        "explorer.confirmDelete": false,
        "javascript.format.semicolons": "insert",
        "cSpell.userWords": [
                "Vuex",
                "firestore",
                "minlength",
                "nuxt",
                "gonuxtjs",
                "socketio",
                "vuexfire"
        ],
        "terminal.external.osxExec": "iTerm.app",
        "terminal.integrated.cursorStyle": "line",
        "terminal.integrated.shell.osx": "/usr/local/bin/zsh",
        "terminal.integrated.fontFamily": "Noto Mono for Powerline",
}