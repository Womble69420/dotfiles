[user]
	name       = ryuta69
	email      = eyma22s.yu@gmail.com
	signingkey = 7EB4C638CC86724B
[core]
	editor = nvim
[color]
	ui = auto
[credential]
	helper = osxkeychain
[alias]
	ad  = add
	ada = add -A
	al  = alias
	br  = branch
	bra = branch -A
	cm  = commit
	co  = checkout
	cob = checkout -b
	df  = diff --word-diff
	lg  = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
	st  = status
 	ps  = !git push origin `git rev-parse --abbrev-ref HEAD`
	pl  = !git pull origin `git rev-parse --abbrev-ref HEAD`
	rb  = rebase
	rf  = reflog --date=iso
	rs  = reset
[commit]
	gpgsign = true
[gpg]
	program = gpg2
[pager]
	log  = diff-highlight | less -RX
	show = diff-highlight | less -RX
	diff = diff-highlight | less -RX
[diff]
	tool                = vimdiff
	algorithm           = histogram
	compactionHeuristic = true
[merge]
	ff = false
[pull]
	ff = only
[ghq]
	root = ~/.ghq
[url "ssh://git@github.com/"]
	insteadOf = https://github.com/
